@article{smithTightLowerBound2019,
	title = {A Tight I/O Lower Bound for Matrix Multiplication},
	url = {http://arxiv.org/abs/1702.02017},
	abstract = {A tight lower bound for required I/O when computing an ordinary matrix-matrix multiplication on a processor with two layers of memory is established. Prior work obtained weaker lower bounds by reasoning about the number of segments needed to perform \$C:={AB}\$, for distinct matrices \$A\$, \$B\$, and \$C\$, where each segment is a series of operations involving \$M\$ reads and writes to and from fast memory, and \$M\$ is the size of fast memory. A lower bound on the number of segments was then determined by obtaining an upper bound on the number of elementary multiplications performed per segment. This paper follows the same high level approach, but improves the lower bound by (1) transforming algorithms for {MMM} so that they perform all computation via fused multiply-add instructions ({FMAs}) and using this to reason about only the cost associated with reading the matrices, and (2) decoupling the per-segment I/O cost from the size of fast memory. For \$n {\textbackslash}times n\$ matrices, the lower bound's leading-order term is \$2n{\textasciicircum}3/{\textbackslash}sqrt\{M\}\$. A theoretical algorithm whose leading terms attains this is introduced. To what extent the state-of-the-art Goto's Algorithm attains the lower bound is discussed.},
	journaltitle = {{arXiv}:1702.02017 [cs]},
	author = {Smith, Tyler Michael and Lowery, Bradley and Langou, Julien and van de Geijn, Robert A.},
	urldate = {2020-01-20},
	date = {2019-02-06},
	langid = {english},
	eprinttype = {arxiv},
	eprint = {1702.02017},
	keywords = {Computer Science - Computational Complexity, Performance Engineering}
}

