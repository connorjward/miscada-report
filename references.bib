@article{goodmanGravothermalOscillations1987,
	title = {On Gravothermal Oscillations},
	volume = {313},
	abstract = {Gravothermal oscillations of postcollapse star clusters are investigated by studying an analogous system, a self-gravitating, conducting gas sphere. The gas is assumed to have a distributed energy source depending on local density and temperature. It is shown that certain functional forms for the energy term permit a new type of self-similar postcollapse solution in which both the core radius and the half-mass radius expand as the two-thirds power of time and the total mass is constant. Creation of binaries in three-body processes leads to one such functional form. Self-similar solutions are exhibited for this interesting case. The solutions are nonsingular and approximately isothermal within the half-mass radius. They are parameterized by the total number of stars, {AT}, and become more centrally condensed with increasing N. The linear stability of these solutions is studied. They are stable for N less than -7000, overstable for N between 7000 and 40,000, and unstable for larger N. For very large {iV}, the instability reduces to the familiar gravothermal instability of an infinite isothermal sphere. The amplitude of nonlinear gravothermal oscillations is estimated and found to increase with N. Implications for globular clusters and for {iV}-body simulations are discussed. It is argued that the linear instability and the nonlinear expansion phase are not directly dependent on the nature of the energy source; rather, they depend on the degree of central concentration in the slowly expanding “equilibrium.” At maximum expansion, the core engulfs {\textasciitilde}l\%-2\% of the total mass. This corresponds to King models having concentration parameters -2-3 and suggests that some very concentrated globular clusters are near the maximum expansion of a postcollapse gravothermal oscillation.},
	pages = {576--595},
	journaltitle = {The Astrophysical Journal},
	author = {Goodman, Jeremy},
	date = {1987-02-15},
	langid = {english}
}

