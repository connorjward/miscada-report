@article{vanhunenContinentalCollisionSlab2011,
	title = {Continental collision and slab break-off: A comparison of 3-D numerical models with observations},
	volume = {302},
	issn = {0012821X},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0012821X10007429},
	doi = {10.1016/j.epsl.2010.11.035},
	shorttitle = {Continental collision and slab break-off},
	abstract = {Conditions and dynamics of subduction–collision and subsequent 3-D slab break-off and slab tear propagation are quantiﬁed, for the ﬁrst time, using fully dynamic numerical models. Model results indicate that collision after the subduction of old, strong subducting oceanic slab leads to slab break-off at 20–25 Myr after the onset of continental collision, and subsequently a slab tear migrates more or less horizontally through the slab with a propagation speed of 100–150 mm/yr. In contrast, young, weak oceanic slabs show the ﬁrst break-off already 10 Myr after continental collision, and can experience tear migration rates up to 800 mm/yr. Slab strength plays a more important role in the timing of slab break-off and the speed of a propagating slab tear than (negative) slab buoyancy does. Slab break-off is viable even for slabs that are supported by the viscosity jump and phase change between the upper and lower mantle.},
	pages = {27--37},
	number = {1},
	journaltitle = {Earth and Planetary Science Letters},
	shortjournal = {Earth and Planetary Science Letters},
	author = {van Hunen, J. and Allen, M. B.},
	urldate = {2020-05-03},
	date = {2011-02},
	langid = {english},
	keywords = {Citcom, subduction}
}

