@article{vesperiniEffectsDynamicalEvolution1997,
	title = {On the effects of dynamical evolution on the initial mass function of globular clusters},
	volume = {289},
	issn = {0035-8711, 1365-2966},
	url = {https://academic.oup.com/mnras/article-lookup/doi/10.1093/mnras/289.4.898},
	doi = {10.1093/mnras/289.4.898},
	abstract = {In this paper we show the results of a large set of 7V-body simulations modelling the evolution of globular clusters driven by relaxation, stellar evolution and disc shocking, and including the effects of the tidal field of the Galaxy. We investigate the evolution of multi-mass models with a power-law initial mass function ({IMF}) starting with different initial masses, concentrations, slopes of the {IMF} and located at different galactocentric distances. We show to what extent the effects of the various evolutionary processes alter the shape of the {IMF} and to what extent these changes depend on the position of the cluster in the Galaxy. The changes in both the global and the local mass functions (measured at different distances from the cluster centre) are investigated, showing whether and where the local mass function keeps memory of the {IMF} and where it provides a good indication of the current global mass function.},
	pages = {898--920},
	number = {4},
	journaltitle = {Monthly Notices of the Royal Astronomical Society},
	author = {Vesperini, E. and Heggie, D. C.},
	urldate = {2019-01-28},
	date = {1997-08},
